{% extends "layout.twig" %}

{% block title %}Index{% endblock %}
{% block head %}
    {{ parent() }}
    <style type="text/css">
        .important { color: #336699; }
    </style>
    <script src="../helper/Mascaras.js" type="text/javascript"></script>
{% endblock %}
{% block content %}
<h4>Inscrição de monitoria</h4>
<div>         
  <form Method ="POST" enctype="multipart/form-data" Action ="?acao=create">  
  <fieldset>
    <div class="container-fluid">
            <input type='hidden' name='id' value='{{idProjeto}}'>
            <label>Qual seu turno disponível para monitoria?</label>
            <select name="listmultiple[]" size="3" multiple="true">
              <option value="1">Manhã</option>
              <option value="2">Tarde</option>
              <option value="4">Noite</option>
            </select>
            <p><input type='submit' class="btn" value="Register" name="enviar">
                 <input name="limpar" class="btn" type="reset" id="limpar" value="Clean">
              <input type="button" class="btn" value="Return" onClick="document.location='../index.php'"> </p>
      </div>
       </fieldset>
       </form>
       </div>    

{% endblock %}